{
  "name": "shared",
  "version": "1.0.0",
  "description": "Shared types, constants, and utilities for PAT Macro Calendar",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./eventMap": {
      "types": "./dist/eventMap.d.ts",
      "import": "./dist/eventMap.js"
    },
    "./sentiment": {
      "types": "./dist/sentiment.d.ts",
      "import": "./dist/sentiment.js"
    },
    "./pairs": {
      "types": "./dist/pairs.d.ts",
      "import": "./dist/pairs.js"
    }
  }
}
